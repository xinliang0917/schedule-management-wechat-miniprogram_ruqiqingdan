.function-item {
  display: flex;
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  background-color: white;
  font-size: 32rpx;
  padding-right: 0;
  border-bottom: solid 5rpx #F0F0F0;
}

.item-text {
  width: 100%;
  text-align: left;
  margin-left: 15rpx;
}

.point {
  vertical-align: middle;
  width: 35rpx;
  height: 35rpx;
  margin: 35rpx;
}